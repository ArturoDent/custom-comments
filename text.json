{
  comments: {
    lineComment: "//********",
    blockComment: [
      "/*",
      "*/",
    ],
  },
  brackets: [
    [
      "{",
      "}",
    ],
    [
      "[",
      "]",
    ],
    [
      "(",
      ")",
    ],
  ],
  wordPattern: "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)",
  indentationRules: {
    increaseIndentPattern: "^((?!.*?\\/\\*).*\\*/)?\\s*[\\}\\]].*$",
    decreaseIndentPattern: "^((?!\\/\\/).)*(\\{[^}\"'`]*|\\([^)\"'`]*|\\[[^\\]\"'`]*)$",
  },
}